<odoo>

    <record model="ir.ui.view" id="stock_procuction_lot_form_view">
        <field
            name="name"
        >stock.production.lot.form (in egd_stock_picking_custom)</field>
        <field name="model">stock.production.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ref']" position="after">
                <field name="egd_certification" />
            </xpath>
        </field>
    </record>



</odoo>
